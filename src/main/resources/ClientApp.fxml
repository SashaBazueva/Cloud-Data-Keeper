<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientApp">
    <children>
        <Label layoutX="208.0" layoutY="35.0" text="Cloud Data Keeper" AnchorPane.leftAnchor="208.0"
               AnchorPane.rightAnchor="207.0" AnchorPane.topAnchor="35.0">
            <font>
                <Font name="Monospaced Regular" size="18.0"/>
            </font>
        </Label>
        <TextArea fx:id="textArea" editable="false" layoutX="24.0" layoutY="130.0" prefHeight="220.0" prefWidth="534.0"
                  AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0"
                  AnchorPane.topAnchor="130.0"/>
        <TextField onAction="#enter" fx:id="textField" layoutX="24.0" layoutY="78.0" prefHeight="25.0" prefWidth="470.0"
                   AnchorPane.bottomAnchor="300.0" AnchorPane.leftAnchor="25.0"/>
        <Button fx:id="button" layoutX="517.0" layoutY="78.0" mnemonicParsing="false" onAction="#enter" text="Enter"
                AnchorPane.bottomAnchor="300.0" AnchorPane.rightAnchor="25.0">
            <font>
                <Font name="Monospaced Italic" size="13.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
